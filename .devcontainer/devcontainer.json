{
	"name": "IceFloeTracker.jl (Debian)",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"features": {
		"ghcr.io/julialang/devcontainer-features/julia:1": {},
		"ghcr.io/devcontainers/features/python:1": {
			"installTools": true,
			"version": "3.12"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.vscode-github-actions",
				"ms-azuretools.vscode-containers"
			]
		}
	},
	"postCreateCommand": {
		"check": "echo 'here is the postcreate'",
		"setup-PyCall": "julia --project=.devcontainer/PyCallSetup.jl .devcontainer/PyCallSetup.jl/setup.jl"
	}
}